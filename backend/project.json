{
  "name": "backend",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "backend/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "nest build",
        "cwd": "backend"
      },
      "configurations": {
        "production": {
          "command": "nest build --webpack",
          "cwd": "backend"
        }
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nest start",
        "cwd": "backend"
      },
      "configurations": {
        "production": {
          "command": "nest start:prod",
          "cwd": "backend"
        }
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nest start --watch",
        "cwd": "backend"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "cwd": "backend"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/coverage/backend"],
      "options": {
        "command": "jest",
        "cwd": "backend"
      }
    },
    "test:e2e": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/coverage/backend"],
      "options": {
        "command": "jest --config ./test/jest-e2e.json",
        "cwd": "backend"
      }
    }
  },
  "tags": ["scope:backend", "type:app"]
}